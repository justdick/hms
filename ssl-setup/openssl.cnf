[req]
default_bits = 2048
prompt = no
default_md = sha256
distinguished_name = dn
x509_extensions = v3_req

[dn]
C = GH
ST = Greater Accra
L = Accra
O = RGH Hospital
OU = IT
CN = 192.168.1.3

[v3_req]
subjectAltName = @alt_names
basicConstraints = CA:FALSE
keyUsage = digitalSignature, keyEncipherment

[alt_names]
IP.1 = 192.168.1.3
IP.2 = 127.0.0.1
DNS.1 = localhost
DNS.2 = rgh.local
